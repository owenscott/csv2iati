

<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title"><%=name%></h3>
  </div>
  <div class="panel-body">

  	<!--main step section-->
  	<div class="row">
  		<div class="col-md-12" id="task">
  		</div>
  	</div>

  	<!--next step nav-->
  	<% if(previous || next) { %>
  		<div class="row">
  			<div class="col-md-4 pull-right">
  				<div class="row">

						<div class="col-md-6">
							<% if (previous) { %>
							<button type="button" id="nav-previous" class="btn btn-primary <% if (!previous.get('enabled')) { %>disabled<%}%>">Previous</button>
							<% } %>
						</div>
  					
						<div class="col-md-6">
							<% if (next) { %>
							<button type="button" id="nav-next" class="btn btn-primary <% if (!next.get('enabled')) { %>disabled<%}%>">Next</button>
							<% } %>
						</div>
  					
  				</div>
  			</div>
  		</div>
  	<%}%>

  </div>
</div>

